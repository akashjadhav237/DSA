using System;
					
public class Program
{
	public static void Main()
	{
		int n = 1;
		var result = IsPowerOfThree(n);
		Console.WriteLine(result);
	}
	public static bool IsPowerOfThree(int n) {
        if (n <= 0)
		{
			return false;
		}
    
    while (n % 3 == 0)
    {
        n /= 3;
    }
    
    return n == 1;
    }
}
